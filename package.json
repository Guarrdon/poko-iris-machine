{
  "name": "poko-iris-machine",
  "version": "1.0.0",
  "description": "new game for nieces",
  "scripts": {
    "grunt": "grunt",
    "start": "node ./dist/server/server.js",
    "server": "tsc -p ./src/server/tsconfig.json",
    "client": "webpack --mode development",
    "full": "tsc -p ./src/server/tsconfig.json | webpack --mode development",
    "test": "jest",
    "build:prod": "webpack -p",
    "watch": "webpack --watch --mode development"
  },
  "author": "ML",
  "license": "ISC",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/styles": "^4.0.2",
    "express": "^4.17.3",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "uuid-random": "^1.0.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "awesome-typescript-loader": "^5.2.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "jest": "^24.8.0",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.2",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typescript": "^3.5.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  }
}
